<?xml version="1.0"?>
<extension key="ca.bidon.civiexportexcel" type="module">
  <file>civiexportexcel</file>
  <name>CiviCRM Export to Excel</name>
  <description>Adds the possibility to export directly to Excel from CiviCRM.</description>
  <license>AGPL3</license>
  <maintainer>
    <author>Mathieu Lutfy</author>
    <email>mathieu@symbiotic.coop</email>
  </maintainer>
  <releaseDate>2020-09-07</releaseDate>
  <version>2.1</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.29</ver>
  </compatibility>
  <comments></comments>
  <urls>
    <url desc="Main Extension Page">https://lab.civicrm.org/extensions/civiexportexcel/</url>
    <url desc="Documentation">https://lab.civicrm.org/extensions/civiexportexcel/blob/master/README.md</url>
    <url desc="Support">https://lab.civicrm.org/extensions/civiexportexcel/issues</url>
    <url desc="Licensing">https://civicrm.org/licensing</url>
  </urls>
  <civix>
    <namespace>CRM/CiviExportExcel</namespace>
  </civix>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
</extension>
